{
  "manifest_version": 2,
  "name": "SwiftTab",
  "description": "Personal new tab page featuring links from your Bookmarks Bar",
  "version": "0.2.0",
  "homepage_url": "https://github.com/joshbuchea/chrome-ext-swifttab",
  "author": "Josh Buchea",
  "permissions": ["bookmarks", "tabs", "menus", "menus.overrideContext"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "assets/icon-16.png",
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  }
}
